/*LEXDEF
INT_LIT : [0-9]+
COMMA := ,
!whitespace : \\s
*/

toplevel ::= int_list(IL). {p->push_root(IL); std::cout << "toplevel" << std::endl; }

int_list(I) ::= . { I = pnn("int_list"); std::cout << "int_list()" << std::endl; }
int_list(G) ::= int_list(IL) COMMA integer(I). { IL->push_back(I); G = IL; std::cout << "int_list(,int)" << std::endl; }

integer(I) ::= INT_LIT(IL). {I = pnn(IL); std::cout << "integer" << std::endl; }