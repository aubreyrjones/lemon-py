/*LEXDEF
INT_LIT : [0-9]+
COMMA := ,
COMMA_BANG := ,!
!comment : ;.*$
!whitespace : \\s
*/

toplevel ::= int_list.

int_list(I) ::= . { I = pnn("int_list"); }
int_list(G) ::= int_list(IL) COMMA integer(I). { IL->push_back(I); G = IL; }
int_list(G) ::= int_list(IL) COMMA_BANG integer(I). { IL->push_back(I); G = IL; }

integer(I) ::= INT_LIT(IL). {I = pnn(IL);}