/*
@pymod     trivial_parser

@lexdef

INT_LIT : [0-9]+
COMMA := ,

!whitespace : \s

@endlex
*/

toplevel ::= int_list(IL). {p->push_root(IL);}

int_list(x_LIST) ::= . { x_LIST = pnn("int_list");}
int_list(x_LIST) ::= int_list(x_RLIST) COMMA integer(I). { x_RLIST->push_back(I); x_LIST = x_RLIST;}

integer(I) ::= INT_LIT(IL). {I = pnn(IL);}